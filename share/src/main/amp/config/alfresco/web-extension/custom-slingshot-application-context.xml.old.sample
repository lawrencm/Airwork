<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans
  PUBLIC '-//SPRING//DTD BEAN//EN'
  'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
   <!-- Add Someco messages -->
   <bean class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent" id="someco.resources">
      <property name="resourceBundles">
         <list>
            <value>alfresco.extension.messages.airworkmodel</value>
         </list>
      </property>
   </bean>
</beans>